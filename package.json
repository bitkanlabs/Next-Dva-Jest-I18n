{
  "name": "create-next-example-app",
  "license": "MIT",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "dev": "next",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@zeit/next-css": "^0.2.0",
    "@zeit/next-typescript": "^1.0.1",
    "antd": "^3.6.1",
    "dva-no-router": "^1.1.3",
    "next": "^6.0.3",
    "next-redux-wrapper": "^2.0.0-beta.6",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/next": "^2.4.11",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^6.0.1",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "useBabelrc": true,
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "testMatch": [
      "**/__tests__/**/*.(t|j)s?(x)",
      "**/?(*.)+(spec|test).(t|j)s?(x)"
    ],
    "collectCoverage": true
  }
}
